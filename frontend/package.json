    "vue": "^3.5.23",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "typescript": "^5.8.0",
    "vite": "^6.0.0",
    "vue-tsc": "^2.2.0"
  }
}
